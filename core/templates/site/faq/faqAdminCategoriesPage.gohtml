{{ template "head" $ }}
<table>
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Question Count</th>
    </tr>
    {{- range .Rows }}
    <tr>
        <td><a href="/admin/faq/categories/category/{{ .ID }}">{{ .ID }}</a></td>
        <td>{{ .Name.String }}</td>
        <td>{{ .Questioncount }}</td>
    </tr>
    {{- end }}
</table>
<p><a href="/admin/faq/categories/new">Create Category</a></p>
{{ template "tail" $ }}
