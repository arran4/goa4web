{{ template "head" $ }}
                <table>
                        <tr>
                                <th>ID
                                <th>Name
                                <th>Question Count
                                <th>Options
                        </tr>
                        {{- range cd.FAQCategoriesWithQuestionCount }}
                        <tr>
                                <td>{{ .Idfaqcategories }}</td>
                                <td>
                                        <form method="post">
        {{ csrfField }}
                                                <input type="hidden" name="cid" value="{{ .Idfaqcategories }}">
                                                <input name="cname" value="{{ .Name.String }}">
                                </td>
                                <td>{{ .Questioncount }}</td>
                                <td>
                                        <input type="submit" name="task" value="Rename Category">
                                        <input type="submit" name="task" value="Delete Category">
                                        </form>
                                </td>
                        </tr>
                        {{- else }}
                        <tr><td colspan="4">No Categories</td></tr>
                        {{- end }}
                        <tr>
                                <td>NEW</td>
                                <td>
                                        <form method="post">
        {{ csrfField }}
                                                <input name="cname" value="">
                                </td>
                                <td></td>
                                <td>
                                        <input type="submit" name="task" value="Create Category">
                                        </form>
                                </td>
                        </tr>
                </table>
{{ template "tail" $ }}