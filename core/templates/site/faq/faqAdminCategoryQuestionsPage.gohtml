{{ template "head" $ }}
<h1>Questions in {{ .Category.Name.String }}</h1>
<table>
    <tr>
        <th>ID</th>
        <th>Question</th>
        <th>Options</th>
    </tr>
    {{- range .Questions }}
    <tr>
        <td><a href="/admin/faq/question/{{ .Idfaq }}/edit">{{ .Idfaq }}</a></td>
        <td>{{ .Question }}</td>
        <td><a href="/faq">View</a> | <a href="/admin/faq/question/{{ .Idfaq }}/edit">Edit</a></td>
    </tr>
    {{- end }}
</table>
{{ template "tail" $ }}
