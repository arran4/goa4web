# admin API signing key (default: )
ADMIN_API_SECRET=
# path to admin API signing key (default: .admin_api_secret)
ADMIN_API_SECRET_FILE=.admin_api_secret
# administrator email addresses (default: )
ADMIN_EMAILS=
# enable admin notification emails (default: true)
ADMIN_NOTIFY=true
# AWS region (default: ) (examples: us-east-1)
AWS_REGION=
# default: 
CONFIG_FILE=
# create missing directories (default: false)
CREATE_DIRS=false
# enable or disable CSRF protection (default: true)
CSRF_ENABLED=true
# database connection string (default: )
DB_CONN=
# database driver (default: mysql)
DB_DRIVER=mysql
# database logging verbosity (default: 0)
DB_LOG_VERBOSITY=0
# default language name (default: )
DEFAULT_LANGUAGE=
# dead letter queue file path (default: )
DLQ_FILE=
# dead letter queue provider (default: )
DLQ_PROVIDER=
# enable sending queued emails (default: true)
EMAIL_ENABLED=true
# default From address (default: )
EMAIL_FROM=
# email logging verbosity (default: 0)
EMAIL_LOG_VERBOSITY=0
# email provider (default: )
EMAIL_PROVIDER=
# email sign off (default: )
EMAIL_SIGNOFF=
# email subject prefix (default: goa4web)
EMAIL_SUBJECT_PREFIX=goa4web
# interval in seconds between email worker runs (default: 60)
EMAIL_WORKER_INTERVAL=60
# enable or disable feeds (default: true)
FEEDS_ENABLED=true
# server base URL (default: http://localhost:8080)
HOSTNAME=http://localhost:8080
# Strict-Transport-Security header value (default: max-age=63072000; includeSubDomains)
HSTS_HEADER=max-age=63072000; includeSubDomains
# directory for cached thumbnails when using the local provider (default: uploads/cache)
IMAGE_CACHE_DIR=uploads/cache
# maximum image cache size in bytes (default: -1)
IMAGE_CACHE_MAX_BYTES=-1
# image cache provider (default: local)
IMAGE_CACHE_PROVIDER=local
# S3 prefix URL for cache (default: ) (examples: s3://mybucket/cache)
IMAGE_CACHE_S3_URL=
# maximum allowed upload size in bytes (default: 52428800)
IMAGE_MAX_BYTES=52428800
# image signing key (default: )
IMAGE_SIGN_SECRET=
# path to image signing key (default: )
IMAGE_SIGN_SECRET_FILE=
# directory to store uploaded images when using the local provider (default: uploads/images)
IMAGE_UPLOAD_DIR=uploads/images
# image upload provider (default: local)
IMAGE_UPLOAD_PROVIDER=local
# S3 prefix URL for uploads (default: ) (examples: s3://mybucket/uploads, s3://bucket/images)
IMAGE_UPLOAD_S3_URL=
# JMAP account (default: )
JMAP_ACCOUNT=
# JMAP endpoint (default: )
JMAP_ENDPOINT=
# JMAP identity (default: )
JMAP_IDENTITY=
# JMAP pass (default: )
JMAP_PASS=
# JMAP user (default: )
JMAP_USER=
# server listen address (default: :8080)
LISTEN=:8080
# failed login attempts allowed within the window (default: 5)
LOGIN_ATTEMPT_THRESHOLD=5
# minutes to track failed login attempts (default: 15)
LOGIN_ATTEMPT_WINDOW=15
# request logging flags (default: 0)
LOG_FLAGS=0
# enable internal notifications (default: true)
NOTIFICATIONS_ENABLED=true
# default page size (default: 15)
PAGE_SIZE_DEFAULT=15
# maximum allowed page size (default: 50)
PAGE_SIZE_MAX=50
# minimum allowed page size (default: 5)
PAGE_SIZE_MIN=5
# password reset expiry in hours (default: 24)
PASSWORD_RESET_EXPIRY_HOURS=24
# SendGrid API key (default: )
SENDGRID_KEY=
# session cookie name (default: my-session)
SESSION_NAME=my-session
# session secret key (default: )
SESSION_SECRET=
# path to session secret file (default: .session_secret)
SESSION_SECRET_FILE=.session_secret
# SMTP auth method (default: plain)
SMTP_AUTH=plain
# SMTP host (default: )
SMTP_HOST=
# SMTP pass (default: )
SMTP_PASS=
# SMTP port (default: )
SMTP_PORT=
# enable or disable STARTTLS (default: true)
SMTP_STARTTLS=true
# SMTP user (default: )
SMTP_USER=
# start year for usage stats (default: 2005)
STATS_START_YEAR=2005
