# The secret key used to sign administrator API tokens. (default: )
ADMIN_API_SECRET=
# The path to a file containing the administrator API signing key. (default: .admin_api_secret)
ADMIN_API_SECRET_FILE=.admin_api_secret
# A comma-separated list of email addresses for administrative notifications. (default: )
ADMIN_EMAILS=
# Enable or disable email notifications for administrators. (default: true)
ADMIN_NOTIFY=true
# Run database migrations on startup. (default: false)
AUTO_MIGRATE=false
# The AWS region to use for SES. (default: ) (examples: us-east-1)
AWS_REGION=
# default: 
CONFIG_FILE=
# Enable or disable the automatic creation of missing directories. (default: false)
CREATE_DIRS=false
# Enable or disable CSRF protection. (default: true)
CSRF_ENABLED=true
# Database connection string. This is used to connect to the database. (default: )
DB_CONN=
# Database driver to use. Supported drivers are 'mysql'. (default: mysql)
DB_DRIVER=mysql
# Database server hostname. (default: )
DB_HOST=
# The verbosity level for database logging. 0 = off, 1 = errors, 2 = warnings, 3 = info, 4 = debug. (default: 0)
DB_LOG_VERBOSITY=0
# Database name. (default: )
DB_NAME=
# Database password. (default: )
DB_PASS=
# Database server port. (default: )
DB_PORT=
# Timezone for the database connection. (default: Australia/Melbourne)
DB_TIMEZONE=Australia/Melbourne
# Database username. (default: )
DB_USER=
# The default language for the application. (default: )
DEFAULT_LANGUAGE=
# The file path for the dead letter queue when using the 'file' provider. (default: )
DLQ_FILE=
# The provider for the dead letter queue. Supported providers are 'file' and 'memory'. (default: )
DLQ_PROVIDER=
# Enable or disable the sending of queued emails. (default: true)
EMAIL_ENABLED=true
# The default 'From' address for outgoing emails. (default: )
EMAIL_FROM=
# The verbosity level for email logging. 0 = off, 1 = errors, 2 = warnings, 3 = info, 4 = debug. (default: 0)
EMAIL_LOG_VERBOSITY=0
# The email provider to use. Supported providers are 'smtp', 'ses', 'jmap', and 'log'. (default: )
EMAIL_PROVIDER=
# A sign-off message to append to the end of all outgoing emails. (default: )
EMAIL_SIGNOFF=
# The prefix to add to the subject of all outgoing emails. (default: goa4web)
EMAIL_SUBJECT_PREFIX=goa4web
# The number of hours an email verification request is valid for. (default: 24)
EMAIL_VERIFICATION_EXPIRY_HOURS=24
# The interval in seconds between runs of the email worker. (default: 60)
EMAIL_WORKER_INTERVAL=60
# Enable or disable RSS/Atom feeds. (default: true)
FEEDS_ENABLED=true
# The base URL of the server, used for generating absolute links. (default: http://localhost:8080)
HOSTNAME=http://localhost:8080
# The value for the Strict-Transport-Security header. (default: max-age=63072000; includeSubDomains)
HSTS_HEADER=max-age=63072000; includeSubDomains
# The directory for cached thumbnails when using the 'local' provider. (default: uploads/cache)
IMAGE_CACHE_DIR=uploads/cache
# The maximum size of the image cache in bytes. A value of -1 means no limit. (default: -1)
IMAGE_CACHE_MAX_BYTES=-1
# The provider to use for the image cache. Supported providers are 'local' and 's3'. (default: local)
IMAGE_CACHE_PROVIDER=local
# The S3 prefix URL for the image cache. (default: ) (examples: s3://mybucket/cache)
IMAGE_CACHE_S3_URL=
# The maximum allowed size for uploaded images in bytes. (default: 52428800)
IMAGE_MAX_BYTES=52428800
# The secret key used to sign image URLs. (default: )
IMAGE_SIGN_SECRET=
# The path to a file containing the image signing key. (default: )
IMAGE_SIGN_SECRET_FILE=
# The directory to store uploaded images when using the 'local' provider. (default: uploads/images)
IMAGE_UPLOAD_DIR=uploads/images
# The provider to use for image uploads. Supported providers are 'local' and 's3'. (default: local)
IMAGE_UPLOAD_PROVIDER=local
# The S3 prefix URL for image uploads. (default: ) (examples: s3://mybucket/uploads, s3://bucket/images)
IMAGE_UPLOAD_S3_URL=
# The account to use for the JMAP server. When omitted the primary mail account from the JMAP session is used. (default: )
JMAP_ACCOUNT=
# The endpoint for the JMAP server. (default: )
JMAP_ENDPOINT=
# The identity to use for the JMAP server. When omitted the default mail identity from the JMAP session is used. (default: )
JMAP_IDENTITY=
# Skip TLS certificate verification for JMAP. (default: false)
JMAP_INSECURE=false
# The password for the JMAP server. (default: )
JMAP_PASS=
# The username for the JMAP server. (default: )
JMAP_USER=
# The secret key used to sign external link URLs. (default: )
LINK_SIGN_SECRET=
# The path to a file containing the external link signing key. (default: )
LINK_SIGN_SECRET_FILE=
# The address and port for the HTTP server to listen on. (default: :8080)
LISTEN=:8080
# The number of failed login attempts allowed within the window before throttling. (default: 5)
LOGIN_ATTEMPT_THRESHOLD=5
# The window in minutes for tracking failed login attempts. (default: 15)
LOGIN_ATTEMPT_WINDOW=15
# The flags for request logging. (default: 0)
LOG_FLAGS=0
# The directory to load migrations from at runtime. (default: )
MIGRATIONS_DIR=
# Enable or disable the internal notification system. (default: true)
NOTIFICATIONS_ENABLED=true
# The height of the generated Open Graph image. (default: 630)
OG_IMAGE_HEIGHT=630
# The width of the generated Open Graph image. (default: 1200)
OG_IMAGE_WIDTH=1200
# The default page size for paginated results. (default: 15)
PAGE_SIZE_DEFAULT=15
# The maximum allowed page size for paginated results. (default: 50)
PAGE_SIZE_MAX=50
# The minimum allowed page size for paginated results. (default: 5)
PAGE_SIZE_MIN=5
# The number of hours a password reset request is valid for. (default: 24)
PASSWORD_RESET_EXPIRY_HOURS=24
# The API key for the SendGrid service. (default: )
SENDGRID_KEY=
# The name of the session cookie. (default: my-session)
SESSION_NAME=my-session
# The SameSite policy for the session cookie. Supported values are 'strict', 'lax', and 'none'. (default: strict)
SESSION_SAME_SITE=strict
# The secret key used to encrypt session data. (default: )
SESSION_SECRET=
# The path to a file containing the session secret key. (default: .session_secret)
SESSION_SECRET_FILE=.session_secret
# The secret key used to sign share URLs. (default: )
SHARE_SIGN_SECRET=
# The path to a file containing the share signing key. (default: )
SHARE_SIGN_SECRET_FILE=
# The authentication method for the SMTP server. (default: plain)
SMTP_AUTH=plain
# The hostname of the SMTP server. (default: )
SMTP_HOST=
# The password for the SMTP server. (default: )
SMTP_PASS=
# The port of the SMTP server. (default: )
SMTP_PORT=
# Enable or disable STARTTLS for SMTP connections. (default: true)
SMTP_STARTTLS=true
# The username for the SMTP server. (default: )
SMTP_USER=
# The start year for usage statistics. (default: 2005)
STATS_START_YEAR=2005
# The directory to load templates from. If not specified, the embedded templates will be used. (default: )
TEMPLATES_DIR=
# The default timezone for the application. (default: Australia/Melbourne)
TIMEZONE=Australia/Melbourne
# The Twitter handle for the site (e.g. @mysite). (default: )
TWITTER_SITE=
