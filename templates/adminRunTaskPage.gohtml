{{ template "head" $ }}
{{- if $.Messages }}
        <ul>
        {{- range $.Messages }}
                <li>{{ . }}</li>
        {{- end }}
        </ul>
{{- end }}
{{- if $.Errors }}
        Errors occurred:
        <ul>
        {{- range $.Errors }}
                <li>{{- .}}</li>
        {{- end }}
        </ul>
{{- else }}
        Task completed successfully.
{{- end }}
<a href="{{$.Back}}">Back</a>
{{ template "tail" $ }}